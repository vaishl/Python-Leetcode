            for i in digits:
                digits[i] = 0
            digits.append(1)
            return reverse(digits)
        elif 9 in digits:
            for i in range(0, len(digits):
                digits2= reversed(digits)
                if digits2[i] == 9 and digits2[i+1] !=9:
                    digits2[i] = 0
                    digits2[i+1] +=1
                elif digits2[i] == 9 and digits2[i+1] == 9:
                    digits2[i] = 0
